@model string
<div id="@Model.ToString()-edit-table" class="modal hide" tabindex="-1" >
    <div class="modal-header no-padding">
        <div class="table-header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            <span id="popup-title"></span>
        </div>
    </div>
    <table class="table table-striped table-bordered table-hover no-margin-bottom no-border-top" id="modalBody">
    </table>
    <div class="modal-footer">
        <button class="btn btn-small btn-danger pull-left" data-dismiss="modal">
            <i class="icon-remove"></i>
            Close
        </button>
        <button class="btn btn-small btn-success pull-right" data-dismiss="savemodal" data-action="reload">
            <i class="icon-save"></i>
            Save
        </button>
    </div>
</div>
